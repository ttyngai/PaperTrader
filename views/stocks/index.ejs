<%- include('../partials/header') %>

<div>
  <h1>Stocks!</h1>
  <form id="new-form" action="/stocks" method="POST">
    <label>Ticker Symbol:</label>
    <input type="text" name="ticker" />
    <input type="submit" value="Add" />
  </form>

  <table id="list">
    <thead>
      <tr>
        <th>Ticker</th>
        <th>% Change</th>
        <th>Bid</th>
        <th>Ask</th>
        <th>Trade</th>
      </tr>
    </thead>
    <tbody>
      <% stocks.forEach(function(s) { %>
      <tr>
        <td><%= s.ticker %></td>
        <td>Some %</td>
        <td>Some $</td>
        <td>Some $</td>
        <td><a href="/stocks/<%= s._id %>">Buy/Sell</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
</div>
<%- include('../partials/footer') %>
